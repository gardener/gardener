config:
  seedConfig:
    apiVersion: core.gardener.cloud/v1beta1
    kind: Seed
    metadata:
      # Automatically set
      name: ""
    spec:
      backup: null
      secretRef: null
      dns:
        provider:
          secretRef:
            # Automatically set when using a Gardener shoot
            name: ""
            namespace: garden
          # Automatically set when using a Gardener shoot
          type: ""
      ingress:
        controller:
          kind: nginx
        # Enter ingress domain of your seed
        domain: ""
      networks:
        blockCIDRs:
        - 169.254.169.254/32
        # Automatically set when using a Gardener shoot
        pods: ""
        # Automatically set when using a Gardener shoot
        nodes: ""
        # Automatically set when using a Gardener shoot
        services: ""
        shootDefaults:
          pods: 100.80.0.0/12
          services: 100.112.0.0/13
      provider:
        # Automatically set when using a Gardener shoot
        region: ""
        # Automatically set when using a Gardener shoot
        type: ""
        # Enter zones of your seed
        zones: []
      settings:
        dependencyWatchdog:
          endpoint:
            enabled: true
          probe:
            enabled: true
        excessCapacityReservation:
          enabled: false
        ownerChecks:
          enabled: false
        scheduling:
          visible: true
        verticalPodAutoscaler:
          enabled: true
